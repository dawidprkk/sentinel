DESCRIPTION >
    Currently blocked accounts in last 5 minutes

NODE endpoint
SQL >
    %
        SELECT
            rule_name,
            sumMerge(hit_count) AS block_count,
            max(bucket) AS last_blocked_at
        FROM decisions_by_rule_10s_mv
        WHERE action = 'block'
          AND bucket >= {{DateTime64(start_time, '2000-01-01 00:00:00.000')}}
        GROUP BY rule_name
        ORDER BY block_count DESC
        LIMIT 20

TYPE endpoint
